<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @element_prototype.name %>
</p>
<p>
  <b>Description:</b>
  <%= @element_prototype.description %>
</p>
<table>
	<% @element_prototype.parameter_def.each do |param| %>
	<tr><td><%= param.name %></td><td><%= param.class %></td></tr>
	<% end %>
</table>


<%= link_to 'Edit', edit_element_prototype_path(@element_prototype) %> |
<%= link_to 'Back', element_prototypes_path %>
